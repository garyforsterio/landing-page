{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "Gary Forster's Landing Page",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel ./src/*.html",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{css,js,json,md,ts,html}\"",
    "prebuild": "rm -rf dist",
    "build": "parcel build ./src/*.html",
    "serve": "serve dist",
    "deploy": "aws s3 sync --delete ./public s3://www.garyforster.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garyforsterio/landing-page.git"
  },
  "author": "Gary Forster <gary@garyforster.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garyforsterio/landing-page/issues"
  },
  "homepage": "https://www.garyforster.io",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.2",
    "serve": "^11.3.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
